<li class=" j_thread_list clearfix" data-field='{&quot;id&quot;:6182486903,&quot;author_name&quot;:&quot;\u534e\u7acb\u8dc6\u62f3\u9053&quot;,&quot;author_nickname&quot;:null,&quot;author_portrait&quot;:&quot;d715e58d8ee7ab8be8b786e68bb3e98193904a&quot;,&quot;first_post_id&quot;:126392150704,&quot;reply_num&quot;:83,&quot;is_bakan&quot;:null,&quot;vid&quot;:&quot;&quot;,&quot;is_good&quot;:null,&quot;is_top&quot;:null,&quot;is_protal&quot;:null,&quot;is_membertop&quot;:null,&quot;is_multi_forum&quot;:null,&quot;frs_tpoint&quot;:null}'  data-tid='6182486903' data-thread-type="0" data-floor='15''>
